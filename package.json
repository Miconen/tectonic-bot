{
    "name": "tectonic-bot",
    "version": "1.2.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "main": "build/main.js",
    "scripts": {
        "build": "swc src -d build",
        "build:changelog": "npx @discordx/changelog --root=src",
        "dev": "ts-node-esm src/main.ts",
        "start": "node build/main.js",
        "watch": "nodemon --exec ts-node-esm src/main.ts"
    },
    "dependencies": {
        "@discordx/importer": "^1.1.10",
        "@discordx/pagination": "^3.2.0",
        "@discordx/utilities": "^5.1.0",
        "@prisma/client": "^4.5.0",
        "@types/node-cron": "^3.0.7",
        "discord.js": "^14.7.1",
        "discordx": "^11.5.2",
        "dotenv": "^16.0.1",
        "eslint": "^8.35.0",
        "node-cron": "^3.0.2"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.3.27",
        "@types/node": "^18.7.18",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "prisma": "^4.11.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    }
}
